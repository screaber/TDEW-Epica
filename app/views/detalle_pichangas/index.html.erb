<h1>Detalle Pichangas</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Idpichanga</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @detalle_pichangas.each do |detalle_pichanga| %>
      <tr>
        <td><%= detalle_pichanga.id %></td>
        <td><%= link_to detalle_pichanga.idpichanga, detalle_pichanga_path(detalle_pichanga) %></td>
        <td><%= detalle_pichanga.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_detalle_pichanga_path(detalle_pichanga), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', detalle_pichanga_path(detalle_pichanga), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_detalle_pichanga_path, :class => 'btn btn-primary' %>
