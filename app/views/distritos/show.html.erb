<h1>Distrito</h1>

<p>
  <b>Nombre</b><br>
  <%= @distrito.nombre %>
</p>

<div class="form-actions">
  <%= link_to 'Back', distritos_path, :class => 'btn'  %>
  <%= link_to 'Edit', edit_distrito_path(@distrito), :class => 'btn' %>
  <%= link_to 'Delete', distrito_path(@distrito), :method => 'delete', :confirm => 'Are you sure?', :class => 'btn btn-danger' %>
</div>
