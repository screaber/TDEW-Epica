<h1>Usuario</h1>

<p>
  <b>Nombre</b><br>
  <%= @usuario.nombre %>
</p>

<p>
  <b>Cuenta</b><br>
  <%= @usuario.cuenta %>
</p>

<div class="form-actions">
  <%= link_to 'Back', usuarios_path, :class => 'btn'  %>
  <%= link_to 'Edit', edit_usuario_path(@usuario), :class => 'btn' %>
  <%= link_to 'Delete', usuario_path(@usuario), :method => 'delete', :confirm => 'Are you sure?', :class => 'btn btn-danger' %>
</div>
